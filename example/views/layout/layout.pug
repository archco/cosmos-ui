doctype html
html
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible' content='IE=edge')
    meta(name='viewport' content='width=device-width, initial-scale=1.0')
    block title
      title "Default title"
    link(rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css')
    link(rel="stylesheet" href="../../dist/css/style.css")
  body
    div(id='app')
      header(class='bg-color-primary pad flex-container-center' style="height:100px;")
        h1(id='project-name') cosmos-css

      nav(class='navbar sticky-top use-activator shadow-near')
        div(class='navbar-header')
          a(class='navbar-brand text' href='index.html') cosmos-css
          button(class='navbar-toggle' type='button') Toggle Navigation
        div(class='navbar-body justify-content-end')
          ul(class='navbar-nav')
            li(class='nav-item')
              a(class='nav-link' href='components.html?q=components') Components
            li(class='nav-item')
              a(class='nav-link' href='modules.html') Modules
            li(class='nav-item')
              a(class='nav-link' href='js-libraries.html') JS Libraries
            li(class='nav-item')
              dropdown(toggle='toggle' align='center')
                button(slot='button') dropdown
                a(href='#') link 1
                a(href='#') link 2
                a(href='#') link 3

      block content

      footer(class='footer')
        div(class='container')
          div(class='row')
            div(class='col-6')
              h3 Footer content
              p footer content here..
            div(class='col-6')
              h3 Links
              ul(class='list-horizontal')
                li
                  a(href='#') Link1
                li
                  a(href='#') Link2
                li
                  a(href='#') Link3
        div(class='footer-copyright')
          |copyright &copy; 2016 cosmos all rights reserved.
      scroll-to(:duration='800' :offset='400' top-html='<i class="fa fa-chevron-up" aria-hidden="true">' bottom-html='<i class="fa fa-chevron-down" aria-hidden="true"></i>')
    <script src="https://cdn.polyfill.io/v2/polyfill.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/babel-polyfill/6.16.0/polyfill.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/vue/2.3.4/vue.js"></script>
    script(src='../../dist/js/script.js')
    script(src='./example.js')
    <script>
    | var year = new Date().getFullYear();
    | document.querySelector('.footer-copyright').innerHTML = "copyright \&copy; " + year + " cosmos all rights reserved.";
    | document.querySelector('#project-name').innerHTML = "cosmos-css " + vm.$cosmos.version;
    </script>
    block script
